{
  "name": "initial-site-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:bdd": "cucumber-js --require-module ts-node/register",
    "test:unit": "vitest run",
    "test:a11y": "vitest run --config vitest.config.a11y.ts",
    "test:keyboard": "node scripts/keyboard-audit.mjs",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist .storybook-storyshots"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.15",
    "@tanstack/react-query": "^5.20.0",
    "axios": "^1.7.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.1.0",
    "@storybook/addon-a11y": "^8.2.0",
    "@storybook/addon-essentials": "^8.2.0",
    "@storybook/react-vite": "^8.2.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "axe-core": "^4.9.1",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "jsdom": "^24.1.0",
    "msw": "^2.3.0",
    "msw-storybook-addon": "^2.0.6",
    "prettier": "^3.3.3",
    "storybook": "^8.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vite-tsconfig-paths": "^5.1.0",
    "vitest": "^1.5.2"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
